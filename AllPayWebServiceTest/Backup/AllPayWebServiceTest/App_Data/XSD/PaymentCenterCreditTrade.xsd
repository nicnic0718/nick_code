<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="PaymentCenterCreditTrade" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
